<?xml version="1.0" encoding="UTF-8"?>

<!-- Fokker Eindecker E III
                  : 2011 Emmanuel BARANGER

     Source       : http://en.wikipedia.org/wiki/Fokker_Eindecker
                 
     Wingspan     : 9.52 m    (  31 ft 2 in )
     Length       : 7.20 m    (  23 ft 6 in )
     Height       : 2.40 m    (   7 ft 9 in )
     Empty weight :  400 kg   ( 882 lb      )
     Engine       : 1 Oberursel U.I 1 Rotary Engine ( 99 hp )
     Max speed    :  140 km/h (  76 kt      )
-->

<airplane mass="882" >

  <approach speed="20" aoa="7" fuel="0.3">
    <control-setting axis="/controls/engines/engine[0]/throttle" value="0.2"/>
    <control-setting axis="/controls/engines/engine[0]/mixture" value="0.65"/>
  </approach>
  
  <cruise speed="70" alt="5000" fuel="0.7" >
    <control-setting axis="/controls/engines/engine[0]/throttle" value="1" />
    <control-setting axis="/controls/engines/engine[0]/mixture" value="0.65" />
  </cruise>

  <cockpit x="1.642" y="0.00" z="0.901"/>

  <fuselage ax="3.366" ay="0" az="0.311" bx="-2.967" by="0" bz="0.314" width="1.031" taper="0.4" midpoint="0.01"/>

  <!-- Wing up -->
  <wing x="1.796" y="0" z="0.329" 
        chord="1.755" 
        length="4.730" 
        taper="1" 
        camber="0.01">
        <stall aoa="24" width="1" peak="1.5"/>
        <flap1 start="0.95" end="1" lift="1.2" drag="1.1"/>
      
        <control-input axis="/controls/flight/aileron" control="FLAP1" split="true"/>
        <control-input axis="/controls/flight/aileron-trim" control="FLAP1" split="true"/>
      
        <control-output control="FLAP1" side="left" prop="/surface-positions/left-aileron-pos-norm"/>
        <control-output control="FLAP1" side="right" prop="/surface-positions/right-aileron-pos-norm"/>

        <control-speed control="FLAP0" transition-time="4"/>
  </wing>

  <hstab x="-3.077" y="0" z="0.309" 
	 chord="0.793" 
	 length="1.444" 
         taper="1"> 
         <stall aoa="22" width="1" peak="1.5"/>
         <flap0 start="0" end="0.7" lift="1.55" drag="1.2"/>

         <control-input axis="/controls/flight/elevator" control="FLAP0"/>
         <control-input axis="/controls/flight/elevator-trim" control="FLAP0"/>

         <control-output control="FLAP0" prop="/surface-positions/elevator-pos-norm"/>

         <control-speed control="FLAP0" transition-time="4"/>
  </hstab>

  <vstab x="-3.157" y="0" z="0.002" 
	 chord="0.864" 
	 length="0.989" 
         taper="1">
         <stall aoa="20" width="1" peak="1.5"/>
         <flap0 start="0" end="1" lift="1.6" drag="1.2"/>

         <control-input axis="/controls/flight/rudder" square="true" control="FLAP0" invert="true"/>
         <control-input axis="/controls/flight/rudder-trim" control="FLAP0" invert="true"/>

         <control-output control="FLAP0" prop="/surface-positions/rudder-pos-norm" min="1" max="-1"/>

         <control-speed control="FLAP0" transition-time="2"/>
  </vstab>

  <!-- Engines      : Oberursel U.I
                      Gnome Monosoupape

       Informations : http://fr.wikipedia.org/wiki/Oberursel_U.I
                      http://en.wikipedia.org/wiki/Gnome_Monosoupape

       Type         : 9-cylinder, single-row, rotary engine
       Power        :   115 hp at 1300 rpm (Maximum power)
       Displacement :  12.8 L     ( 781.63 in3 )
       Diameter     :    95 cm    (  37.40 in  )
       Masse        : 137.4 kg    ( 303.00 lb  )
  -->
  <propeller x="3.218" y="0" z="0.281"  
             radius="1.21"
             mass="303"       
             moment="4"
             cruise-power="100"
             cruise-alt="5000" 
             cruise-speed="70"
             cruise-rpm="1100"
             takeoff-power="115"
             takeoff-rpm="1300">
             <actionpt x="3.506" y="0" z="0.281"/>
             <piston-engine eng-rpm="1300" 
                            alt="5000" 
                            eng-power="115"
                            displacement="781.63"/>
             <control-input control="THROTTLE" axis="/controls/engines/engine[0]/throttle"/>
             <control-input control="STARTER" axis="/controls/engines/engine[0]/starter"/>
             <control-input control="MAGNETOS" axis="/controls/engines/engine[0]/magnetos"/>
             <control-input control="MIXTURE" axis="/controls/engines/engine[0]/mixture"/>
  </propeller>

  <!-- Left wheel -->
  <gear x="2.531" y="1.010" z="-1.197" 
        sfric="1.1"
        dfric="0.6"
        compression="0.02">
  </gear>

  <!-- Right wheel -->
  <gear x="2.531" y="-1.010" z="-1.197" 
        sfric="1.1"
        dfric="0.6"
        compression="0.02">
  </gear>

  <!-- Tail wheel -->
  <gear x="-3.138" y="0" z="-0.195" 
        sfric="1.1"
        dfric="0.6"
        compression="0.02">
  </gear>

  <!-- Fuel: Forward 142 lbs -->
  <tank x="2.824" y="0" z="0.562" capacity="160"/>
  
</airplane>
